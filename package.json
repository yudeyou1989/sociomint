{
  "name": "sociomint",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:filenames": "eslint --ext .js,.jsx,.ts,.tsx --rule 'filename-rules/match: error' .",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:unit": "jest --testPathPattern=\"simple|formatters\"",
    "test:integration": "jest --testPathPattern=\"integration\"",
    "test:components": "jest --testPathPattern=\"components\"",
    "test:performance": "jest --testPathPattern=\"performance\"",
    "test:contracts": "npx hardhat test",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "test:visual": "playwright test visual-regression",
    "test:all": "npm run test:ci && npm run e2e",
    "playwright:install": "playwright install",
    "docs:serve": "npx serve docs",
    "env:validate": "node scripts/manage-env-vars.js --action=validate",
    "env:sync:dev": "node scripts/manage-env-vars.js --action=sync --env=development",
    "env:sync:prod": "node scripts/manage-env-vars.js --action=sync --env=production",
    "env:create:template": "node scripts/manage-env-vars.js --action=template",
    "env:check": "node scripts/manage-env-vars.js --action=check",
    "cache:clear": "rm -rf .next && rm -rf node_modules/.cache",
    "db:setup": "node scripts/setup-supabase.js",
    "deploy": "node ../scripts/deploy-vercel.js",
    "deploy:prod": "node ../scripts/deploy-vercel.js --prod",
    "deploy:contracts": "npx hardhat run scripts/deploy.js --network bscTestnet",
    "db:deploy": "node scripts/deploy-database.js",
    "db:reset": "node scripts/deploy-database.js --reset",
    "db:seed": "node scripts/deploy-database.js --seed",
    "db:migrate": "node scripts/deploy-database.js --migrate-only",
    "db:status": "node scripts/deploy-database.js --status",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:gen-types": "supabase gen types typescript --local > src/types/supabase.ts",
    "deploy:airdrop-pool:testnet": "npx hardhat run scripts/deploy-airdrop-pool.js --network bscTestnet",
    "deploy:airdrop-pool:mainnet": "npx hardhat run scripts/deploy-airdrop-pool.js --network bsc",
    "init:airdrop-pool:testnet": "npx hardhat run scripts/initialize-airdrop-pool.js --network bscTestnet",
    "init:airdrop-pool:mainnet": "npx hardhat run scripts/initialize-airdrop-pool.js --network bsc",
    "airdrop:fund:testnet": "npx hardhat run scripts/initialize-airdrop-pool.js --network bscTestnet -- --fund",
    "airdrop:test:testnet": "npx hardhat run scripts/initialize-airdrop-pool.js --network bscTestnet -- --test-data --run-tests",
    "telegram:start": "node src/lib/telegram/airdropBot.js",
    "upgrade:sm-exchange:testnet": "npx hardhat run scripts/upgrade-sm-token-exchange.js --network bscTestnet",
    "upgrade:sm-exchange:mainnet": "npx hardhat run scripts/upgrade-sm-token-exchange.js --network bsc",
    "verify:sm-exchange:testnet": "npx hardhat verify --network bscTestnet",
    "verify:sm-exchange:mainnet": "npx hardhat verify --network bsc",
    "db:migrate:daily-rewards": "supabase db push --file supabase/migrations/007_daily_rewards_schema.sql",
    "telegram:daily-rewards": "node src/lib/telegram/dailyRewardBot.js",
    "check:deployment": "node scripts/check-deployment-readiness.js",
    "deploy:check": "npm run check:deployment",
    "setup:github-secrets": "node scripts/setup-github-secrets.js",
    "secrets:setup": "npm run setup:github-secrets",
    "upload:github": "bash scripts/upload-to-github.sh",
    "github:upload": "npm run upload:github"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.72.0",
    "@types/dompurify": "^3.0.5",
    "@wagmi/connectors": "^5.8.3",
    "@walletconnect/ethereum-provider": "^2.19.2",
    "@walletconnect/modal": "^2.7.0",
    "@web3modal/wagmi": "^3.5.7",
    "cross-fetch": "3.1.5",
    "dompurify": "^3.2.6",
    "ethers": "^6.11.1",
    "i18next": "^25.0.1",
    "i18next-browser-languagedetector": "^8.0.5",
    "i18next-http-backend": "^3.0.2",
    "next": "15.2.4",
    "node-fetch": "^2.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.3",
    "viem": "^2.29.4",
    "wagmi": "^2.15.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^29.7.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19.1.5",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "eslint-plugin-filename-rules": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pino-pretty": "^13.0.0",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.0.0",
    "typescript": "^5",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  }
}