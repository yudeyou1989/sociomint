{
  "name": "sociomint",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:cf": "./scripts/build-production.sh",
    "build:prod": "./scripts/build-production.sh",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "npm run analyze && npx serve .next/analyze",
    "start": "next start",
    "preview": "npm run build && npx serve out",
    "lint": "next lint",
    "lint:filenames": "eslint --ext .js,.jsx,.ts,.tsx --rule 'filename-rules/match: error' .",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:unit": "jest --testPathPattern=\"simple|formatters\"",
    "test:integration": "jest --testPathPattern=\"integration\"",
    "test:components": "jest --testPathPattern=\"components\"",
    "test:performance:unit": "jest --testPathPattern=\"performance\"",
    "test:contracts": "npx hardhat test",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "test:visual": "playwright test visual-regression",
    "test:all": "npm run test:ci && npm run e2e",
    "playwright:install": "playwright install",
    "docs:serve": "npx serve docs",
    "env:validate": "node scripts/manage-env-vars.js --action=validate",
    "env:sync:dev": "node scripts/manage-env-vars.js --action=sync --env=development",
    "env:sync:prod": "node scripts/manage-env-vars.js --action=sync --env=production",
    "env:create:template": "node scripts/manage-env-vars.js --action=template",
    "env:check": "node scripts/manage-env-vars.js --action=check",
    "cache:clear": "rm -rf .next && rm -rf node_modules/.cache",
    "db:setup": "node scripts/setup-supabase.js",
    "security:check": "node scripts/final-security-check.js",
    "performance:test": "node scripts/performance-benchmark.js",
    "config:validate": "node scripts/production-config-validator.js",
    "deploy:check": "node scripts/deployment-readiness-check.js",
    "deploy:ready": "npm run deploy:check && echo '✅ 项目已准备好部署！'",
    "test:performance": "node scripts/simple-performance-test.js",
    "test:compatibility": "node scripts/compatibility-test.js",
    "test:automated": "node scripts/automated-testing-suite.js",
    "deploy": "npm run build",
    "deploy:prod": "npm run build",
    "deploy:contracts": "npx hardhat run scripts/deploy.js --network bscTestnet",
    "db:deploy": "node scripts/deploy-database.js",
    "db:reset": "node scripts/deploy-database.js --reset",
    "db:seed": "node scripts/deploy-database.js --seed",
    "db:migrate": "node scripts/deploy-database.js --migrate-only",
    "db:status": "node scripts/deploy-database.js --status",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:gen-types": "supabase gen types typescript --local > src/types/supabase.ts",
    "deploy:airdrop-pool:testnet": "npx hardhat run scripts/deploy-airdrop-pool.js --network bscTestnet",
    "deploy:airdrop-pool:mainnet": "npx hardhat run scripts/deploy-airdrop-pool.js --network bsc",
    "init:airdrop-pool:testnet": "npx hardhat run scripts/initialize-airdrop-pool.js --network bscTestnet",
    "init:airdrop-pool:mainnet": "npx hardhat run scripts/initialize-airdrop-pool.js --network bsc",
    "airdrop:fund:testnet": "npx hardhat run scripts/initialize-airdrop-pool.js --network bscTestnet -- --fund",
    "airdrop:test:testnet": "npx hardhat run scripts/initialize-airdrop-pool.js --network bscTestnet -- --test-data --run-tests",
    "telegram:start": "node src/lib/telegram/airdropBot.js",
    "upgrade:sm-exchange:testnet": "npx hardhat run scripts/upgrade-sm-token-exchange.js --network bscTestnet",
    "upgrade:sm-exchange:mainnet": "npx hardhat run scripts/upgrade-sm-token-exchange.js --network bsc",
    "verify:sm-exchange:testnet": "npx hardhat verify --network bscTestnet",
    "verify:sm-exchange:mainnet": "npx hardhat verify --network bsc",
    "db:migrate:daily-rewards": "supabase db push --file supabase/migrations/007_daily_rewards_schema.sql",
    "telegram:daily-rewards": "node src/lib/telegram/dailyRewardBot.js",
    "production:check": "node scripts/production-readiness-check.js",
    "production:ready": "npm run production:check",
    "setup:github-secrets": "node scripts/setup-github-secrets.js",
    "secrets:setup": "npm run setup:github-secrets",
    "upload:github": "bash scripts/upload-to-github.sh",
    "github:upload": "npm run upload:github",
    "deploy:cf": "node scripts/deploy-cloudflare.js --deploy",
    "deploy:cf:full": "node scripts/deploy-cloudflare.js --full",
    "preview:cf": "node scripts/deploy-cloudflare.js --preview",
    "clean:cf": "node scripts/deploy-cloudflare.js --clean",
    "check:quick": "node scripts/quick-check.js",
    "lint:check": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "security-check": "node scripts/security-check.js",
    "quality-check": "npm run lint && npm run type-check && npm run test:unit",
    "pre-commit": "npm run quality-check"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@sentry/nextjs": "^9.40.0",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.72.0",
    "@types/dompurify": "^3.0.5",
    "@wagmi/connectors": "^5.8.3",
    "@walletconnect/ethereum-provider": "^2.19.2",
    "@walletconnect/modal": "^2.7.0",
    "@web3modal/wagmi": "^5.1.0",
    "dompurify": "^3.2.6",
    "ethers": "^6.11.1",
    "i18next": "^25.3.0",
    "isomorphic-dompurify": "^2.26.0",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.5.3",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.3",
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "twitter-api-v2": "^1.23.2",
    "viem": "^2.29.4",
    "wagmi": "^2.15.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^29.7.0",
    "@next/bundle-analyzer": "^15.3.5",
    "@playwright/test": "^1.52.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.0",
    "@types/react": "^19",
    "@types/react-dom": "^19.1.5",
    "autoprefixer": "^10.4.16",
    "babel-plugin-import": "^1.13.8",
    "chrome-launcher": "^1.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "eslint-plugin-filename-rules": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.8.0",
    "pino-pretty": "^13.0.0",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "typescript": "^5"
  },
  "optionalDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8"
  }
}
