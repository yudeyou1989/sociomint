        (cov_16pfqv5xu7().b[33][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "bg-red-900/30 text-red-400 p-3 rounded-md text-sm",
          children: errorMessage
        })),
        /* istanbul ignore next */
        (cov_16pfqv5xu7().b[34][0]++, successMessage) &&
        /* istanbul ignore next */
        (cov_16pfqv5xu7().b[34][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "bg-green-900/30 text-green-400 p-3 rounded-md text-sm",
          children: successMessage
        })), /*#__PURE__*/(0, _jsxruntime.jsx)(Button, {
          onClick: handleExchange,
          disabled:
          /* istanbul ignore next */
          (cov_16pfqv5xu7().b[35][0]++, isLoading) ||
          /* istanbul ignore next */
          (cov_16pfqv5xu7().b[35][1]++, !isConnected) ||
          /* istanbul ignore next */
          (cov_16pfqv5xu7().b[35][2]++, parseFloat(fromAmount) <= 0),
          children: isLoading ?
          /* istanbul ignore next */
          (/*#__PURE__*/cov_16pfqv5xu7().b[36][0]++, (0, _jsxruntime.jsxs)("div", {
            className: "flex items-center justify-center",
            children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("svg", {
              className: "animate-spin -ml-1 mr-2 h-4 w-4 text-white",
              xmlns: "http://www.w3.org/2000/svg",
              fill: "none",
              viewBox: "0 0 24 24",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)("circle", {
                className: "opacity-25",
                cx: "12",
                cy: "12",
                r: "10",
                stroke: "currentColor",
                strokeWidth: "4"
              }), /*#__PURE__*/(0, _jsxruntime.jsx)("path", {
                className: "opacity-75",
                fill: "currentColor",
                d: "M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z"
              })]
            }), t('common.loading')]
          })) :
          /* istanbul ignore next */
          (cov_16pfqv5xu7().b[36][1]++, !isConnected ?
          /* istanbul ignore next */
          (cov_16pfqv5xu7().b[37][0]++, t('wallet.connect')) :
          /* istanbul ignore next */
          (cov_16pfqv5xu7().b[37][1]++, exchangeMode === 'BNB_TO_SM' ?
          /* istanbul ignore next */
          (cov_16pfqv5xu7().b[38][0]++, t('exchange.exchangeToSm')) :
          /* istanbul ignore next */
          (cov_16pfqv5xu7().b[38][1]++, t('exchange.exchangeToBnb'))))
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "text-center text-xs text-gray-500",
          children: t('exchange.gasFeeNotice')
        })]
      })]
    }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "bg-gray-900 rounded-xl p-6 shadow-xl border border-gray-800",
      children: /*#__PURE__*/(0, _jsxruntime.jsx)(_XPlatformVerification.default, {
        onVerificationSuccess: handleVerificationSuccess
      })
    })]
  });
}
