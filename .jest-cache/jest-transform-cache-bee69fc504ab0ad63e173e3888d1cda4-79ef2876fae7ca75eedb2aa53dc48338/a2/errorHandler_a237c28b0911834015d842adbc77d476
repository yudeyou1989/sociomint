  }
  // 记录错误
  logError(error) {
    /* istanbul ignore next */
    cov_g1fa156xo().f[17]++;
    cov_g1fa156xo().s[54]++;
    this.errorLog.unshift(error);
    // 限制日志大小
    /* istanbul ignore next */
    cov_g1fa156xo().s[55]++;
    if (this.errorLog.length > this.maxLogSize) {
      /* istanbul ignore next */
      cov_g1fa156xo().b[19][0]++;
      cov_g1fa156xo().s[56]++;
      this.errorLog = this.errorLog.slice(0, this.maxLogSize);
    } else
    /* istanbul ignore next */
    {
      cov_g1fa156xo().b[19][1]++;
    }
    // 控制台输出
    cov_g1fa156xo().s[57]++;
    if (
    /* istanbul ignore next */
    (cov_g1fa156xo().b[21][0]++, error.severity === 'critical') ||
    /* istanbul ignore next */
    (cov_g1fa156xo().b[21][1]++, error.severity === 'high')) {
      /* istanbul ignore next */
      cov_g1fa156xo().b[20][0]++;
      cov_g1fa156xo().s[58]++;
      console.error('AppError:', error);
    } else {
      /* istanbul ignore next */
      cov_g1fa156xo().b[20][1]++;
      cov_g1fa156xo().s[59]++;
      console.warn('AppError:', error);
    }
    // 发送到监控服务（如果配置了）
    /* istanbul ignore next */
    cov_g1fa156xo().s[60]++;
    this.sendToMonitoring(error);
  }
  // 发送到监控服务
  sendToMonitoring(error) {
    /* istanbul ignore next */
    cov_g1fa156xo().f[18]++;
    cov_g1fa156xo().s[61]++;
    // 这里可以集成Sentry、LogRocket等监控服务
    if (
    /* istanbul ignore next */
    (cov_g1fa156xo().b[23][0]++, typeof window !== 'undefined') &&
    /* istanbul ignore next */
    (cov_g1fa156xo().b[23][1]++, window.Sentry)) {
      /* istanbul ignore next */
      cov_g1fa156xo().b[22][0]++;
      cov_g1fa156xo().s[62]++;
      window.Sentry.captureException(new Error(error.message), {
        tags: {
          errorCode: error.code,
          severity: error.severity,
          category: error.category
        },
        extra: {
          details: error.details,
          metadata: error.metadata
        }
      });
    } else
    /* istanbul ignore next */
    {
      cov_g1fa156xo().b[22][1]++;
    }
  }
  // 获取错误日志
  getErrorLog() {
    /* istanbul ignore next */
