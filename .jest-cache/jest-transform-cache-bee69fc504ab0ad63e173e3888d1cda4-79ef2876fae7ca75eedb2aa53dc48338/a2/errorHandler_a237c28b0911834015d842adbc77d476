const handleComponentError = (error, errorInfo) => {
  /* istanbul ignore next */
  cov_g1fa156xo().f[24]++;
  const handler =
  /* istanbul ignore next */
  (cov_g1fa156xo().s[75]++, ErrorHandler.getInstance());
