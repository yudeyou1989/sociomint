const withErrorHandling = (fn, errorHandler) => {
  /* istanbul ignore next */
  cov_g1fa156xo().f[25]++;
  cov_g1fa156xo().s[79]++;
  return async (...args) => {
    /* istanbul ignore next */
    cov_g1fa156xo().f[26]++;
    cov_g1fa156xo().s[80]++;
    try {
      /* istanbul ignore next */
      cov_g1fa156xo().s[81]++;
      return await fn(...args);
    } catch (error) {
      const handler =
      /* istanbul ignore next */
      (cov_g1fa156xo().s[82]++, ErrorHandler.getInstance());
      const appError =
      /* istanbul ignore next */
      (cov_g1fa156xo().s[83]++, errorHandler ?
      /* istanbul ignore next */
      (cov_g1fa156xo().b[25][0]++, errorHandler(error)) :
      /* istanbul ignore next */
      (cov_g1fa156xo().b[25][1]++, handler.createError(ERROR_CODES.INTERNAL_ERROR, error)));
      /* istanbul ignore next */
      cov_g1fa156xo().s[84]++;
      throw appError;
    }
  };
};
/* istanbul ignore next */
cov_g1fa156xo().s[85]++;
const withRetry = async (fn, maxRetries =
/* istanbul ignore next */
(cov_g1fa156xo().b[26][0]++, 3), delay =
/* istanbul ignore next */
(cov_g1fa156xo().b[27][0]++, 1000), backoff =
/* istanbul ignore next */
(cov_g1fa156xo().b[28][0]++, 2)) => {
  /* istanbul ignore next */
  cov_g1fa156xo().f[27]++;
  let lastError;
  /* istanbul ignore next */
  cov_g1fa156xo().s[86]++;
  for (let i =
  /* istanbul ignore next */
  (cov_g1fa156xo().s[87]++, 0); i <= maxRetries; i++) {
    /* istanbul ignore next */
    cov_g1fa156xo().s[88]++;
    try {
      /* istanbul ignore next */
      cov_g1fa156xo().s[89]++;
      return await fn();
    } catch (error) {
      /* istanbul ignore next */
      cov_g1fa156xo().s[90]++;
      lastError = error;
      // 如果是不可重试的错误，直接抛出
      /* istanbul ignore next */
      cov_g1fa156xo().s[91]++;
      if (error.retryable === false) {
        /* istanbul ignore next */
        cov_g1fa156xo().b[29][0]++;
        cov_g1fa156xo().s[92]++;
        throw error;
      } else
      /* istanbul ignore next */
      {
        cov_g1fa156xo().b[29][1]++;
      }
      // 如果已经是最后一次尝试，抛出错误
      cov_g1fa156xo().s[93]++;
      if (i === maxRetries) {
        /* istanbul ignore next */
        cov_g1fa156xo().b[30][0]++;
        cov_g1fa156xo().s[94]++;
        throw error;
      } else
      /* istanbul ignore next */
      {
        cov_g1fa156xo().b[30][1]++;
      }
      // 等待后重试
      cov_g1fa156xo().s[95]++;
      await new Promise(resolve => {
        /* istanbul ignore next */
        cov_g1fa156xo().f[28]++;
        cov_g1fa156xo().s[96]++;
        return setTimeout(resolve, delay * Math.pow(backoff, i));
      });
    }
  }
  /* istanbul ignore next */
  cov_g1fa156xo().s[97]++;
  throw lastError;
};
