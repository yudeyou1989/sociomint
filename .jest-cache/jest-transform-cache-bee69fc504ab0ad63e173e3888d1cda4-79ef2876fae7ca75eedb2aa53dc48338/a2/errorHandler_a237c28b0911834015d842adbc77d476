  // 处理验证错误
  handleValidationError(error, field) {
    /* istanbul ignore next */
    cov_g1fa156xo().f[16]++;
    const metadata =
    /* istanbul ignore next */
    (cov_g1fa156xo().s[48]++, field ?
    /* istanbul ignore next */
    (cov_g1fa156xo().b[16][0]++, {
      field
    }) :
    /* istanbul ignore next */
    (cov_g1fa156xo().b[16][1]++, undefined));
    /* istanbul ignore next */
