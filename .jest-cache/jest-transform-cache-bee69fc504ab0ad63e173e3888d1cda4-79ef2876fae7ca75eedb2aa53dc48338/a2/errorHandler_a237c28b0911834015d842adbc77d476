  }
  // 获取错误统计
  getErrorStats() {
    /* istanbul ignore next */
    cov_g1fa156xo().f[21]++;
    const stats =
    /* istanbul ignore next */
    (cov_g1fa156xo().s[65]++, {
      total: this.errorLog.length,
      bySeverity: {
        critical: 0,
        high: 0,
        medium: 0,
        low: 0
      },
      byCategory: {
        network: 0,
        validation: 0,
        auth: 0,
        contract: 0,
        system: 0,
        user: 0
      },
      retryable: 0,
      recent: this.errorLog.slice(0, 10)
    });
    /* istanbul ignore next */
    cov_g1fa156xo().s[66]++;
    this.errorLog.forEach(error => {
      /* istanbul ignore next */
      cov_g1fa156xo().f[22]++;
      cov_g1fa156xo().s[67]++;
      stats.bySeverity[error.severity]++;
      /* istanbul ignore next */
      cov_g1fa156xo().s[68]++;
      stats.byCategory[error.category]++;
      /* istanbul ignore next */
      cov_g1fa156xo().s[69]++;
      if (error.retryable) {
        /* istanbul ignore next */
        cov_g1fa156xo().b[24][0]++;
        cov_g1fa156xo().s[70]++;
        stats.retryable++;
      } else
      /* istanbul ignore next */
      {
        cov_g1fa156xo().b[24][1]++;
      }
    });
    /* istanbul ignore next */
    cov_g1fa156xo().s[71]++;
    return stats;
  }
  constructor() {
    /* istanbul ignore next */
    cov_g1fa156xo().f[23]++;
    cov_g1fa156xo().s[72]++;
    this.errorLog = [];
    /* istanbul ignore next */
    cov_g1fa156xo().s[73]++;
    this.maxLogSize = 100;
  }
}
