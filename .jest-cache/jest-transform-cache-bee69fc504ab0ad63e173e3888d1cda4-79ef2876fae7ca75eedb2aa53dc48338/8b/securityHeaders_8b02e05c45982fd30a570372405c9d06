  } else
  /* istanbul ignore next */
  {
    cov_1o14oyrvtx().b[8][1]++;
  }
  cov_1o14oyrvtx().s[58]++;
  return input.replace(/[<>]/g, '') // HTML
  .replace(/javascript:/gi, '') // JavaScript
  .replace(/on\w+=/gi, '') // 
  .trim().slice(0, 1000); // 
}
