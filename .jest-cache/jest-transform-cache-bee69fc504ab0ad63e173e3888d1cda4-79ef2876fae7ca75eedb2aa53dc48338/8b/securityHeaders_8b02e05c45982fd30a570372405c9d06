function preventSQLInjection(query) {
  /* istanbul ignore next */
  cov_1o14oyrvtx().f[21]++;
  const sqlInjectionPatterns =
  /* istanbul ignore next */
  (cov_1o14oyrvtx().s[67]++, [/(\b(SELECT|INSERT|UPDATE|DELETE|DROP|CREATE|ALTER|EXEC|UNION|SCRIPT)\b)/i, /(--|\/\*|\*\/|;|'|"|`)/, /(\bOR\b|\bAND\b).*?[=<>]/i]);
