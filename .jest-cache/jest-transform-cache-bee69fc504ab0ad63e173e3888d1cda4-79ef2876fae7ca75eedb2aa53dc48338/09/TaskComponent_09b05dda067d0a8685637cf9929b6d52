        })))]
      })]
    }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "bg-gray-50 px-5 py-3 border-t border-gray-200",
      children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "flex items-center justify-between",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "flex items-center",
          children: /*#__PURE__*/(0, _jsxruntime.jsxs)("span", {
            className: "text-sm text-gray-600",
            children: ["奖励: ", task.reward_amount, " ", task.reward_type === 'sm_token' ?
            /* istanbul ignore next */
            (cov_1uf9jszhkt().b[89][0]++, 'SM') :
            /* istanbul ignore next */
            (cov_1uf9jszhkt().b[89][1]++, task.reward_type)]
          })
        }), /*#__PURE__*/(0, _jsxruntime.jsxs)("span", {
          className: "text-sm text-gray-500",
          children: ["截止日期: ", new Date(task.end_date).toLocaleDateString()]
        })]
      })
    })]
  });
};
const _default =
/* istanbul ignore next */
(cov_1uf9jszhkt().s[139]++, TaskPanel);
