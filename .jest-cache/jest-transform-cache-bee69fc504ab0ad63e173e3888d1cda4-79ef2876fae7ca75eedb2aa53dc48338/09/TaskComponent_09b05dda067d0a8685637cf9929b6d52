      }
    } else
    /* istanbul ignore next */
    {
      cov_1uf9jszhkt().b[11][1]++;
    }
  }
  /* istanbul ignore next */
  cov_1uf9jszhkt().s[29]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_1uf9jszhkt().s[30]++;
  if (cache) {
    /* istanbul ignore next */
    cov_1uf9jszhkt().b[16][0]++;
    cov_1uf9jszhkt().s[31]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_1uf9jszhkt().b[16][1]++;
  }
  cov_1uf9jszhkt().s[32]++;
