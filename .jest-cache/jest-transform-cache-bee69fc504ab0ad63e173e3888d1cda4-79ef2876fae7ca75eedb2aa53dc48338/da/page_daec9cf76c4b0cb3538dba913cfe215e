    } else
    /* istanbul ignore next */
    {
      cov_11klxgdk4n().b[11][1]++;
    }
  }
  /* istanbul ignore next */
  cov_11klxgdk4n().s[30]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_11klxgdk4n().s[31]++;
  if (cache) {
    /* istanbul ignore next */
    cov_11klxgdk4n().b[16][0]++;
    cov_11klxgdk4n().s[32]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_11klxgdk4n().b[16][1]++;
  }
  cov_11klxgdk4n().s[33]++;
  return newObj;
}
