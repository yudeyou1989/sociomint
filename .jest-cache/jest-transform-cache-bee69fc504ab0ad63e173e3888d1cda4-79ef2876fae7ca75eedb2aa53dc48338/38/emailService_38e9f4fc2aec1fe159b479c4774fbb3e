      /* istanbul ignore next */
      cov_2b2vtr16hz().f[12]++;
      cov_2b2vtr16hz().s[43]++;
      return {
        filename: att.filename,
        content: Buffer.isBuffer(att.content) ?
        /* istanbul ignore next */
        (cov_2b2vtr16hz().b[17][0]++, att.content.toString('base64')) :
        /* istanbul ignore next */
        (cov_2b2vtr16hz().b[17][1]++, att.content),
        type:
        /* istanbul ignore next */
        (cov_2b2vtr16hz().b[18][0]++, att.type) ||
        /* istanbul ignore next */
        (cov_2b2vtr16hz().b[18][1]++, 'application/octet-stream'),
        disposition: 'attachment'
      };
    })
  });
  const [response] =
  /* istanbul ignore next */
  (cov_2b2vtr16hz().s[44]++, await _mail.default.send(msg));
  /* istanbul ignore next */
  cov_2b2vtr16hz().s[45]++;
  return {
    success: true,
    messageId: response.headers['x-message-id']
  };
}
