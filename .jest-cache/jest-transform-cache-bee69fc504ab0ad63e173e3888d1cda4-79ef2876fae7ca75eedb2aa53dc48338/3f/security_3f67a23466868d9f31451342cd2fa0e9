function getClientIP(request) {
  /* istanbul ignore next */
  cov_2lfoe52ro6().f[6]++;
  const forwarded =
  /* istanbul ignore next */
  (cov_2lfoe52ro6().s[12]++, request.headers.get('x-forwarded-for'));
  const realIP =
  /* istanbul ignore next */
  (cov_2lfoe52ro6().s[13]++, request.headers.get('x-real-ip'));
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[14]++;
  if (forwarded) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[0][0]++;
    cov_2lfoe52ro6().s[15]++;
    return forwarded.split(',')[0].trim();
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[0][1]++;
  }
  cov_2lfoe52ro6().s[16]++;
  if (realIP) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[1][0]++;
    cov_2lfoe52ro6().s[17]++;
    return realIP;
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[1][1]++;
  }
  cov_2lfoe52ro6().s[18]++;
  return /* istanbul ignore next */(cov_2lfoe52ro6().b[2][0]++, request.ip) ||
  /* istanbul ignore next */
  (cov_2lfoe52ro6().b[2][1]++, 'unknown');
}
function rateLimitMiddleware(request) {
  /* istanbul ignore next */
  cov_2lfoe52ro6().f[7]++;
  const pathname =
  /* istanbul ignore next */
  (cov_2lfoe52ro6().s[19]++, request.nextUrl.pathname);
  const clientIP =
  /* istanbul ignore next */
  (cov_2lfoe52ro6().s[20]++, getClientIP(request));
  // 获取对应的速率限制配置
  let rateLimit =
  /* istanbul ignore next */
  (cov_2lfoe52ro6().s[21]++, RATE_LIMITS.default);
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[22]++;
  for (const [path, limit] of Object.entries(RATE_LIMITS)) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().s[23]++;
    if (
    /* istanbul ignore next */
    (cov_2lfoe52ro6().b[4][0]++, path !== 'default') &&
    /* istanbul ignore next */
    (cov_2lfoe52ro6().b[4][1]++, pathname.startsWith(path))) {
      /* istanbul ignore next */
      cov_2lfoe52ro6().b[3][0]++;
      cov_2lfoe52ro6().s[24]++;
      rateLimit = limit;
      /* istanbul ignore next */
      cov_2lfoe52ro6().s[25]++;
      break;
    } else
    /* istanbul ignore next */
    {
      cov_2lfoe52ro6().b[3][1]++;
    }
  }
  // 检查速率限制
  const rateLimitKey =
  /* istanbul ignore next */
  (cov_2lfoe52ro6().s[26]++, `${clientIP}:${pathname}`);
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[27]++;
  if (_security.RateLimiter.isRateLimited(rateLimitKey, rateLimit.maxRequests, rateLimit.windowMs)) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[5][0]++;
    cov_2lfoe52ro6().s[28]++;
    return _server.NextResponse.json({
      error: 'Too many requests',
      code: 'RATE_LIMIT_EXCEEDED'
    }, {
      status: 429
    });
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[5][1]++;
  }
  cov_2lfoe52ro6().s[29]++;
  return null;
}
function csrfProtectionMiddleware(request) {
  /* istanbul ignore next */
  cov_2lfoe52ro6().f[8]++;
  cov_2lfoe52ro6().s[30]++;
  // 只对POST、PUT、DELETE请求进行CSRF检查
  if (!['POST', 'PUT', 'DELETE'].includes(request.method)) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[6][0]++;
    cov_2lfoe52ro6().s[31]++;
    return null;
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[6][1]++;
  }
  const csrfToken =
  /* istanbul ignore next */
  (cov_2lfoe52ro6().s[32]++, request.headers.get('x-csrf-token'));
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[33]++;
  if (
  /* istanbul ignore next */
  (cov_2lfoe52ro6().b[8][0]++, !csrfToken) ||
  /* istanbul ignore next */
  (cov_2lfoe52ro6().b[8][1]++, !_security.CSRFProtection.validateToken(csrfToken))) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[7][0]++;
    cov_2lfoe52ro6().s[34]++;
    return _server.NextResponse.json({
      error: 'Invalid CSRF token',
      code: 'CSRF_TOKEN_INVALID'
    }, {
      status: 403
    });
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[7][1]++;
  }
  cov_2lfoe52ro6().s[35]++;
  return null;
}
async function inputValidationMiddleware(request) {
  /* istanbul ignore next */
  cov_2lfoe52ro6().f[9]++;
  cov_2lfoe52ro6().s[36]++;
  if (!['POST', 'PUT', 'PATCH'].includes(request.method)) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[9][0]++;
    cov_2lfoe52ro6().s[37]++;
    return null;
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[9][1]++;
  }
  cov_2lfoe52ro6().s[38]++;
  try {
    const body =
    /* istanbul ignore next */
    (cov_2lfoe52ro6().s[39]++, await request.json());
    const pathname =
    /* istanbul ignore next */
    (cov_2lfoe52ro6().s[40]++, request.nextUrl.pathname);
    // 根据不同的API路径进行相应的验证
    /* istanbul ignore next */
    cov_2lfoe52ro6().s[41]++;
    if (pathname.includes('/api/wallet')) {
      /* istanbul ignore next */
      cov_2lfoe52ro6().b[10][0]++;
      cov_2lfoe52ro6().s[42]++;
      return validateWalletRequest(body);
    } else {
      /* istanbul ignore next */
      cov_2lfoe52ro6().b[10][1]++;
      cov_2lfoe52ro6().s[43]++;
      if (pathname.includes('/api/exchange')) {
        /* istanbul ignore next */
        cov_2lfoe52ro6().b[11][0]++;
        cov_2lfoe52ro6().s[44]++;
        return validateExchangeRequest(body);
      } else {
        /* istanbul ignore next */
        cov_2lfoe52ro6().b[11][1]++;
        cov_2lfoe52ro6().s[45]++;
        if (pathname.includes('/api/tasks')) {
          /* istanbul ignore next */
          cov_2lfoe52ro6().b[12][0]++;
          cov_2lfoe52ro6().s[46]++;
          return validateTaskRequest(body);
        } else
        /* istanbul ignore next */
        {
          cov_2lfoe52ro6().b[12][1]++;
        }
      }
    }
    /* istanbul ignore next */
    cov_2lfoe52ro6().s[47]++;
    return null;
  } catch (error) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().s[48]++;
    return _server.NextResponse.json({
      error: 'Invalid JSON payload',
      code: 'INVALID_JSON'
    }, {
      status: 400
    });
  }
}
/**
 * 验证钱包相关请求
 */
function validateWalletRequest(body) {
  /* istanbul ignore next */
  cov_2lfoe52ro6().f[10]++;
  cov_2lfoe52ro6().s[49]++;
  if (
  /* istanbul ignore next */
  (cov_2lfoe52ro6().b[14][0]++, body.address) &&
  /* istanbul ignore next */
  (cov_2lfoe52ro6().b[14][1]++, !_security.InputValidator.validateWalletAddress(body.address))) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[13][0]++;
    cov_2lfoe52ro6().s[50]++;
    return _server.NextResponse.json({
      error: 'Invalid wallet address format',
      code: 'INVALID_WALLET_ADDRESS'
    }, {
      status: 400
    });
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[13][1]++;
  }
  cov_2lfoe52ro6().s[51]++;
  if (
  /* istanbul ignore next */
  (cov_2lfoe52ro6().b[16][0]++, body.amount) &&
  /* istanbul ignore next */
  (cov_2lfoe52ro6().b[16][1]++, !_security.InputValidator.validateNumber(body.amount))) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[15][0]++;
    cov_2lfoe52ro6().s[52]++;
    return _server.NextResponse.json({
      error: 'Invalid amount format',
      code: 'INVALID_AMOUNT'
    }, {
      status: 400
    });
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[15][1]++;
  }
  cov_2lfoe52ro6().s[53]++;
  return null;
}
/**
 * 验证交换相关请求
 */
function validateExchangeRequest(body) {
  /* istanbul ignore next */
  cov_2lfoe52ro6().f[11]++;
  cov_2lfoe52ro6().s[54]++;
  if (body.bnbAmount) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[17][0]++;
    cov_2lfoe52ro6().s[55]++;
    if (!_security.InputValidator.validateNumber(body.bnbAmount)) {
      /* istanbul ignore next */
      cov_2lfoe52ro6().b[18][0]++;
      cov_2lfoe52ro6().s[56]++;
      return _server.NextResponse.json({
        error: 'Invalid BNB amount format',
        code: 'INVALID_BNB_AMOUNT'
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_2lfoe52ro6().b[18][1]++;
    }
    const amount =
    /* istanbul ignore next */
    (cov_2lfoe52ro6().s[57]++, parseFloat(body.bnbAmount));
    /* istanbul ignore next */
    cov_2lfoe52ro6().s[58]++;
    if (!_security.InputValidator.validateRange(amount, 0.001, 100)) {
      /* istanbul ignore next */
      cov_2lfoe52ro6().b[19][0]++;
      cov_2lfoe52ro6().s[59]++;
      return _server.NextResponse.json({
        error: 'BNB amount out of valid range',
        code: 'AMOUNT_OUT_OF_RANGE'
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_2lfoe52ro6().b[19][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[17][1]++;
  }
  cov_2lfoe52ro6().s[60]++;
  if (body.slippage) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[20][0]++;
    cov_2lfoe52ro6().s[61]++;
    if (!_security.InputValidator.validateNumber(body.slippage)) {
      /* istanbul ignore next */
      cov_2lfoe52ro6().b[21][0]++;
      cov_2lfoe52ro6().s[62]++;
      return _server.NextResponse.json({
        error: 'Invalid slippage format',
        code: 'INVALID_SLIPPAGE'
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_2lfoe52ro6().b[21][1]++;
    }
    const slippage =
    /* istanbul ignore next */
    (cov_2lfoe52ro6().s[63]++, parseFloat(body.slippage));
    /* istanbul ignore next */
    cov_2lfoe52ro6().s[64]++;
    if (!_security.InputValidator.validateRange(slippage, 0.1, 50)) {
      /* istanbul ignore next */
      cov_2lfoe52ro6().b[22][0]++;
      cov_2lfoe52ro6().s[65]++;
      return _server.NextResponse.json({
        error: 'Slippage out of valid range (0.1% - 50%)',
        code: 'SLIPPAGE_OUT_OF_RANGE'
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_2lfoe52ro6().b[22][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[20][1]++;
  }
  cov_2lfoe52ro6().s[66]++;
  return null;
}
/**
 * 验证任务相关请求
 */
function validateTaskRequest(body) {
  /* istanbul ignore next */
  cov_2lfoe52ro6().f[12]++;
  cov_2lfoe52ro6().s[67]++;
  if (
  /* istanbul ignore next */
  (cov_2lfoe52ro6().b[24][0]++, body.url) &&
  /* istanbul ignore next */
  (cov_2lfoe52ro6().b[24][1]++, !_security.InputValidator.validateUrl(body.url))) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[23][0]++;
    cov_2lfoe52ro6().s[68]++;
    return _server.NextResponse.json({
      error: 'Invalid URL format',
      code: 'INVALID_URL'
    }, {
      status: 400
    });
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[23][1]++;
  }
  cov_2lfoe52ro6().s[69]++;
  if (
  /* istanbul ignore next */
  (cov_2lfoe52ro6().b[26][0]++, body.hashtag) &&
  /* istanbul ignore next */
  (cov_2lfoe52ro6().b[26][1]++, !_security.InputValidator.validateHashtag(body.hashtag))) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[25][0]++;
    cov_2lfoe52ro6().s[70]++;
    return _server.NextResponse.json({
      error: 'Invalid hashtag format',
      code: 'INVALID_HASHTAG'
    }, {
      status: 400
    });
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[25][1]++;
  }
  cov_2lfoe52ro6().s[71]++;
  if (
  /* istanbul ignore next */
  (cov_2lfoe52ro6().b[28][0]++, body.content) &&
  /* istanbul ignore next */
  (cov_2lfoe52ro6().b[28][1]++, !_security.InputValidator.validateLength(body.content, 1, 5000))) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[27][0]++;
    cov_2lfoe52ro6().s[72]++;
    return _server.NextResponse.json({
      error: 'Content length must be between 1 and 5000 characters',
      code: 'INVALID_CONTENT_LENGTH'
    }, {
      status: 400
    });
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[27][1]++;
  }
  cov_2lfoe52ro6().s[73]++;
  return null;
}
function securityHeadersMiddleware(response) {
  /* istanbul ignore next */
  cov_2lfoe52ro6().f[13]++;
  cov_2lfoe52ro6().s[74]++;
  // 设置安全头部
  response.headers.set('X-Content-Type-Options', 'nosniff');
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[75]++;
  response.headers.set('X-Frame-Options', 'DENY');
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[76]++;
  response.headers.set('X-XSS-Protection', '1; mode=block');
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[77]++;
  response.headers.set('Referrer-Policy', 'strict-origin-when-cross-origin');
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[78]++;
  response.headers.set('Permissions-Policy', 'camera=(), microphone=(), geolocation=()');
  // 设置CSP头部
  const csp =
  /* istanbul ignore next */
  (cov_2lfoe52ro6().s[79]++, ["default-src 'self'", "script-src 'self' 'unsafe-inline' 'unsafe-eval' https://vercel.live", "style-src 'self' 'unsafe-inline' https://fonts.googleapis.com", "font-src 'self' https://fonts.gstatic.com", "img-src 'self' data: https:", "connect-src 'self' https: wss:", "frame-ancestors 'none'", "base-uri 'self'", "form-action 'self'"].join('; '));
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[80]++;
  response.headers.set('Content-Security-Policy', csp);
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[81]++;
  return response;
}
async function securityMiddleware(request) {
  /* istanbul ignore next */
  cov_2lfoe52ro6().f[14]++;
  // 1. 速率限制检查
  const rateLimitResponse =
  /* istanbul ignore next */
  (cov_2lfoe52ro6().s[82]++, rateLimitMiddleware(request));
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[83]++;
  if (rateLimitResponse) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[29][0]++;
    cov_2lfoe52ro6().s[84]++;
    return securityHeadersMiddleware(rateLimitResponse);
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[29][1]++;
  }
  // 2. CSRF保护检查
  const csrfResponse =
  /* istanbul ignore next */
  (cov_2lfoe52ro6().s[85]++, csrfProtectionMiddleware(request));
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[86]++;
  if (csrfResponse) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[30][0]++;
    cov_2lfoe52ro6().s[87]++;
    return securityHeadersMiddleware(csrfResponse);
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[30][1]++;
  }
  // 3. 输入验证检查
  const validationResponse =
  /* istanbul ignore next */
  (cov_2lfoe52ro6().s[88]++, await inputValidationMiddleware(request));
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[89]++;
  if (validationResponse) {
    /* istanbul ignore next */
    cov_2lfoe52ro6().b[31][0]++;
    cov_2lfoe52ro6().s[90]++;
    return securityHeadersMiddleware(validationResponse);
  } else
  /* istanbul ignore next */
  {
    cov_2lfoe52ro6().b[31][1]++;
  }
  // 4. 继续处理请求
  const response =
  /* istanbul ignore next */
  (cov_2lfoe52ro6().s[91]++, _server.NextResponse.next());
  // 5. 添加安全头部
  /* istanbul ignore next */
  cov_2lfoe52ro6().s[92]++;
  return securityHeadersMiddleware(response);
}
/**
 * 清理速率限制记录的定时任务
 */
/* istanbul ignore next */
cov_2lfoe52ro6().s[93]++;
if (typeof window === 'undefined') {
  /* istanbul ignore next */
  cov_2lfoe52ro6().b[32][0]++;
  cov_2lfoe52ro6().s[94]++;
  setInterval(() => {
    /* istanbul ignore next */
    cov_2lfoe52ro6().f[15]++;
    cov_2lfoe52ro6().s[95]++;
    _security.RateLimiter.cleanup();
  }, 60000); // 每分钟清理一次
} else
/* istanbul ignore next */
{
  cov_2lfoe52ro6().b[32][1]++;
}
